<html>
  <head>
    <meta charset="utf-8"> 
  </head>
  <body id="body">  
    <%= form_for :nation , url: nations_path do |n| %>
    <p>
      <%= n.label :english %><br>
      <%= n.text_field :english %>
    </p>
    <p>
      <%= n.label :chinese %><br>
      <%= n.text_field :chinese %>
    </p>
    <p>
      <%= n.label :pinyin %><br>
      <%= n.text_field :pinyin %>
    </p> 
    <p>
      <%= n.submit %>
    </p>
    <% end %>
  
    
  </body>
</html>
