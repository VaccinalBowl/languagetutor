{
    "name": "Russian Builder",
    "description": "Add on to look for new words that you may not know.",
    "version": "0.1",
    "permissions": ["tabs","webNavigation","contextMenus", "http://*/*", "https://*/*"],
    "browser_action": {
	"default_title": "Search this page for Russian Words",
	"default_icon": "images/icon.png",
	"default_popup":"html/popup.html"
    },
    "background": {
	"scripts": ["scripts/background.js"]
    },
    "icons":{
	"16":"images/icon.png"
    },
    "manifest_version":2
}
